                "medication_id": {
                  "type": "string"
                },
                "form": {
                  "type": "string",
                  "description": "`Dictionary MEDICATION_FORM`"
                },
                "dosage": {
                  "type": "object",
                  "properties": {
                    "numerator_unit": {
                      "type": "string",
                      "description": "`Dictionary MEDICATION_UNIT`"
                    },
                    "numerator_value": {
                      "type": "number"
                    },
                    "denumerator_unit": {
                      "type": "string",
                      "description": "`Dictionary MEDICATION_UNIT`"
                    },
                    "denumerator_value": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "numerator_unit",
                    "numerator_value",
                    "denumerator_unit",
                    "denumerator_value"
                  ]
                },
                "container": {
                  "type": "object",
                  "properties": {
                    "numerator_unit": {
                      "type": "string",
                      "description": "`Dictionary MEDICATION_UNIT`"
                    },
                    "numerator_value": {
                      "type": "number"
                    },
                    "denumerator_unit": {
                      "type": "string",
                      "description": "`Dictionary MEDICATION_UNIT`"
                    },
                    "denumerator_value": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "numerator_unit",
                    "numerator_value",
                    "denumerator_unit",
                    "denumerator_value"
                  ]
                },
                "medication_qty": {
                  "type": "number",
                  "description": "Medication quantity isuued by the doctor"
                }
              },
              "required": [
                "innm",
                "medication_id",
                "form",
                "dosage",
                "container",
                "medication_qty"
              ]
            },
            "medical_program": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
