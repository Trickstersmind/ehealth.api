                        "description": "`Dictionary MEDICATION_UNIT`"
                      },
                      "numerator_value": {
                        "type": "number",
                        "enum": [
                          1
                        ]
                      },
                      "denumerator_unit": {
                        "type": "string",
                        "enum": [
                          "PILL"
                        ],
                        "description": "`Dictionary MEDICATION_UNIT`"
                      },
                      "denumerator_value": {
                        "type": "number",
                        "enum": [
                          1
                        ]
                      }
                    },
