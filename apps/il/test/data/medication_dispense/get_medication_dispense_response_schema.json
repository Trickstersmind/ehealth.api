            "type": "object",
            "properties": {
              "medication": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "enum": [
                      "Амідарон"
                    ]
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "MEDICATION"
                    ]
                  },
                  "manufacturer": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "enum": [
                          "ПАТ \"Київський вітамінний завод\""
                        ]
                      },
                      "country": {
                        "type": "string",
                        "enum": [
                          "UA"
                        ],
                        "description": "`Dictionary COUNTRY`"
                      }
                    },
                    "required": [
                      "name",
                      "country"
                    ],
                    "additionalProperties": false
                  },
                  "form": {
                    "type": "string",
                    "enum": [
                      "PILL"
