        },
        "division": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "legal_entity_id": {
              "type": "string",
              "description": "6c54-4b01-90e6-d701748f0851 (string, required)"
            },
            "type": {
              "type": "string",
              "enum": [
                "CLINIC",
                "AMBULANT_CLINIC",
                "FAP"
